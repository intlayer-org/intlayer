{
  "name": "next-intlayer",
  "version": "2.0.13",
  "private": false,
  "description": "Internationalisation tool for Next.js applications. Declare your multilingual contant in the same lever than your component. Powered by TypeScript, declaration files.",
  "keywords": [
    "intlayer",
    "multilingual",
    "i18n",
    "internationalization",
    "nextjs",
    "typescript",
    "react",
    "json"
  ],
  "homepage": "https://intlayer.org",
  "bugs": {
    "url": "https://github.com/aymericzip/intlayer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymericzip/intlayer.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Aymeric PINEAU",
    "url": "https://github.com/aymericzip"
  },
  "contributors": [
    {
      "name": "Aymeric Pineau",
      "email": "ay.pineau@gmail.com",
      "url": "https://github.com/aymericzip"
    }
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/mjs/index.mjs"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "require": "./dist/cjs/client/index.cjs",
      "import": "./dist/mjs/client/index.mjs"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "require": "./dist/cjs/middleware/index.cjs",
      "import": "./dist/mjs/middleware/index.mjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "require": "./dist/cjs/server/index.cjs",
      "import": "./dist/mjs/server/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/mjs/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "package.json": [
        "./package.json"
      ]
    }
  },
  "files": [
    "./dist",
    "./package.json"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rimraf ./dist",
    "dev": "vite build --watch",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.5.4",
    "@intlayer/chokidar": "workspace:^",
    "@intlayer/config": "workspace:^",
    "@intlayer/core": "workspace:^",
    "@intlayer/design-system": "workspace:^",
    "@intlayer/webpack": "workspace:^",
    "intlayer": "workspace:^",
    "negotiator": "^0.6.3",
    "next": "14.2.15",
    "react-intlayer": "workspace:^",
    "webpack": "^5.93.0"
  },
  "devDependencies": {
    "@types/negotiator": "^0.6.3",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@utils/eslint-config": "workspace:^",
    "@utils/ts-config": "workspace:^",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.11.1",
    "prettier": "3.3.3",
    "react": "^18.3.1",
    "rimraf": "6.0.1",
    "rollup-preserve-directives": "^1.1.2",
    "typescript": "^5.5.4",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.1.2"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "engines": {
    "node": ">=14.18"
  },
  "bug": {
    "url": "https://github.com/aymericzip/intlayer/issues"
  }
}
