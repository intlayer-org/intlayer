{
  "name": "@intlayer/cli",
  "version": "2.0.13",
  "private": false,
  "description": "Command Line Interface for IntLayer - CLI process to start the applications server.",
  "keywords": [
    "intlayer",
    "cli",
    "command",
    "line",
    "interface",
    "server"
  ],
  "homepage": "https://intlayer.org",
  "bugs": {
    "url": "https://github.com/aymericzip/intlayer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymericzip/intlayer.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Aymeric PINEAU",
    "url": "https://github.com/aymericzip"
  },
  "contributors": [
    {
      "name": "Aymeric Pineau",
      "email": "ay.pineau@gmail.com",
      "url": "https://github.com/aymericzip"
    }
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "man": "./man/intlayer.1",
  "files": [
    "./dist",
    "./package.json"
  ],
  "scripts": {
    "build": "vite build",
    "clean": "rimraf ./dist",
    "dev": "vite build --watch",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --cache --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@intlayer/chokidar": "workspace:^",
    "@intlayer/config": "workspace:^",
    "@intlayer/design-system": "workspace:^",
    "@intlayer/dictionaries-entry": "workspace:^",
    "commander": "^12.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@intlayer/backend": "workspace:^",
    "@intlayer/core": "workspace:^",
    "@types/lodash": "^4.17.10",
    "@types/node": "^20.14.9",
    "@utils/ts-config": "workspace:^",
    "eslint": "^9.11.1",
    "prettier": "3.3.3",
    "rimraf": "5.0.5",
    "rollup-preserve-directives": "^1.1.2",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.1.2"
  },
  "bug": {
    "url": "https://github.com/aymericzip/intlayer/issues"
  }
}
